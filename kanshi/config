profile undocked {
	output eDP-1 enable mode 1920x1080@60.012Hz position 0,0 scale 1 
	output DP-3 disable
	output DP-5 disable
}

profile docked {
	output eDP-1 disable
	output DP-3 enable mode 1920x1080@60.000Hz position 0,0 scale 1
	output DP-5 enable mode 1920x1080@60.000Hz position 3840,0 scale 1
}
